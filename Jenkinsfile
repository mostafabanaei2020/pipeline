pipeline {
   agent any
   
   stages {
  stage('name') {
    steps {
      echo 'my name is mostafa'
    }
  }

  stage('family') {
    steps {
      echo 'my family is banaei'
    }
  }

  stage('email') {
    steps {
      echo 'my email address  is mostafa.banaeo2020@gmail.com'
    }
  }

  stage('mobile') {
    steps {
      echo 'my name is 09379979274'
    }
  }

}


}
